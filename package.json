{
  "name": "react-vis",
  "version": "0.0.1",
  "engines": {
    "node": ">=5.0 <6",
    "npm": ">=3.0 <4"
  },
  "description": "Using react and some vis libs(like d3) to visualization data.",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint src",
    "stylelint": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint": "npm run eslint && npm run stylelint",
    "start": "npm run eslint && node server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangmingkai4315/react-vis.git"
  },
  "keywords": [
    "react",
    "visualization",
    "d3"
  ],
  "author": "Mike",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhangmingkai4315/react-vis/issues"
  },
  "homepage": "https://github.com/zhangmingkai4315/react-vis#readme",
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single"
    }
  },
  "babel": {
    "presets": [
      "react",
      "node5",
      "stage-0"
    ],
    "env": {
      "development": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.8.0",
    "browser-sync": "^2.12.8",
    "browser-sync-webpack-plugin": "^1.0.1",
    "eslint": "^2.10.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "stylelint": "^6.3.3",
    "stylelint-config-standard": "^7.0.0",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-middleware": "^1.5.1"
  },
  "dependencies": {
    "d3": "^3.5.17",
    "express": "^4.13.4",
    "morgan": "^1.7.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "uuid": "^2.0.2"
  }
}
